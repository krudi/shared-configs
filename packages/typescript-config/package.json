{
    "$schema": "https://json.schemastore.org/package.json",
    "name": "@krudi/typescript-config",
    "version": "0.1.9",
    "private": false,
    "description": "Shared TypeScript configuration for projects to provide consistent compiler settings.",
    "keywords": [
        "typescript",
        "tsconfig",
        "config",
        "configuration",
        "compiler-options",
        "shared-config",
        "monorepo",
        "nextjs",
        "expo",
        "base-config",
        "build"
    ],
    "homepage": "https://github.com/krudi/shared-configs#readme",
    "bugs": {
        "url": "https://github.com/krudi/shared-configs/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/krudi/shared-configs.git"
    },
    "license": "MIT",
    "author": {
        "name": "krudi",
        "email": "patryk.kudlik@gmail.com",
        "url": "https://github.com/krudi"
    },
    "sideEffects": false,
    "type": "module",
    "files": [
        "dist",
        "README.md",
        "LICENSE",
        "external-package.json",
        "internal-package.json",
        "base.json",
        "expo.json",
        "nextjs.json"
    ],
    "scripts": {
        "lint:prettier": "prettier --check . --cache --cache-strategy metadata --cache-location .cache/.prettiercache",
        "lint:prettier:fix": "prettier --write . --cache --cache-strategy metadata --cache-location .cache/.prettiercache"
    },
    "engines": {
        "node": ">= 22.18.0"
    }
}
